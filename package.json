{
  "name": "reactivatoradmin",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "react-scripts start",
    "dev": "CHOKIDAR_USEPOLLING=true react-scripts start",
    "build": "react-scripts build",
    "test": "CI=true react-scripts test --env=jsdom --detectOpenHandles --forceExit",
    "coverage": "CI=true react-scripts test --env=jsdom --coverage",
    "coveralls": "yarn coverage && cat ./coverage/lcov.info | node node_modules/.bin/coveralls",
    "eject": "react-scripts eject",
    "lint": "tslint -p tsconfig.json 'src/**/*.{ts,tsx}'"
  },
  "dependencies": {
    "@types/jest": "^23.3.9",
    "@types/node": "^10.12.3",
    "@types/react": "^16.7.1",
    "@types/react-dom": "^16.0.9",
    "bulma": "^0.7.2",
    "bulma-extensions": "^4.0.0",
    "dotenv": "^6.1.0",
    "firebase": "^5.5.9",
    "node-sass": "^4.10.0",
    "re-base": "^4.0.0",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "react-redux": "^6.0.0",
    "react-scripts": "2.1.2",
    "redux": "^4.0.1",
    "redux-firestore": "^0.6.0",
    "redux-saga": "^0.16.2",
    "typescript": "^3.1.6"
  },
  "devDependencies": {
    "@types/dotenv": "^6.1.0",
    "@types/enzyme": "^3.1.15",
    "@types/re-base": "^3.2.0",
    "@types/react-redux": "^6.0.11",
    "coveralls": "^3.0.2",
    "enzyme": "^3.8.0",
    "enzyme-adapter-react-16": "^1.7.1",
    "tslint": "^5.11.0",
    "tslint-config-standard": "^8.0.1",
    "tslint-react": "^3.6.0"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx}",
      "!src/index.tsx",
      "!src/serviceWorker.js"
    ]
  }
}
